{
  "name": "seasoned-evaluator",
  "version": "0.0.2",
  "description": "Simple UI component to add and read reviews.",
  "author": "SeasonedSoftware",
  "license": "MIT",
  "repository": "SeasonedSoftware/seasoned-evaluator",
  "main": "dist/seasoned-evaluator.cjs.js",
  "module": "dist/seasoned-evaluator.esm.js",
  "types": "dist/types/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "scaffold": "node ./scripts/scaffold.js",
    "build": "npm run build:types && npm run build:js",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "yarn build && cd example/ && yarn build && cd ../",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.1.2",
    "@material-ui/icons": "^4.2.1",
    "lodash": "^4.17.x",
    "prop-types": "15.x",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@material-ui/core": "^4.1.3",
    "@material-ui/icons": "^4.2.1",
    "@types/lodash": "^4.14.134",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "cross-env": "^5.2.0",
    "gh-pages": "^2.0.1",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "readline-promise": "^1.0.4",
    "replace-in-file": "^4.1.0",
    "rollup": "^1.16.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-uglify": "^6.0.2",
    "typescript": "^3.5.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {}
}
